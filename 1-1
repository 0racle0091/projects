
#include <iostream>

int main()
{
    using namespace std;
    int a,b;
    cout << "vvedite koordinati tochki (x,y)";
    cin >> a >> b;
    if (a == 0) {
        if (b == 0) {
            cout << "0";
        }
        else {
            cout << "1";
        }
    }
    else {
        if (b == 0) {
            cout << "2";
        }
        else {
            cout << "3";
        }
    }
    system("pause");
    return 0;
}

